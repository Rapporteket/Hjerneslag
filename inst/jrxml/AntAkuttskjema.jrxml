	<queryString language="SQL">
		<![CDATA[SELECT
Avdeling,count(DataSettID),sum(related)
FROM
(SELECT Avdeling,DataSettID, case RelatedID WHEN 0>RelatedID THEN 0 ELSE 1 END AS related
FROM HjerneSlagPROD
where HjerneSlagPROD.`Avdeling` IS NOT NULL) temp
group by  Avdeling]]>
	</queryString>
